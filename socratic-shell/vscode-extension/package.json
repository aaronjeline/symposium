{
  "name": "socratic-shell",
  "displayName": "Socratic Shell",
  "description": "",
  "version": "0.1.0",
  "publisher": "socratic-shell",
  "repository": {
    "type": "git",
    "url": "https://github.com/socratic-shell/symposium.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "socratic-shell.showReview",
        "title": "Show Review",
        "category": "Socratic Shell"
      },
      {
        "command": "socratic-shell.copyReview",
        "title": "Copy Review",
        "category": "Socratic Shell",
        "icon": "$(copy)"
      },
      {
        "command": "socratic-shell.logPIDs",
        "title": "Log PIDs",
        "category": "Socratic Shell"
      },
      {
        "command": "socratic-shell.showFileDiff",
        "title": "Show File Diff",
        "category": "Socratic Shell"
      },
      {
        "command": "socratic-shell.addComment",
        "title": "Add Comment",
        "category": "Socratic Shell"
      },
      {
        "command": "socratic-shell.addCommentReply",
        "title": "Add Comment Reply",
        "category": "Socratic Shell"
      },
      {
        "command": "socratic-shell.addWalkthroughComment",
        "title": "Add Walkthrough Comment",
        "category": "Socratic Shell"
      },
      {
        "command": "socratic-shell.toggleComments",
        "title": "Toggle Comments",
        "category": "Socratic Shell",
        "icon": "$(comment)"
      },
      {
        "command": "socratic-shell.toggleWindowTitle",
        "title": "Toggle Window Title",
        "category": "Socratic Shell"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "socratic-shell",
          "title": "Socratic Shell",
          "icon": "activitybar-logo.svg"
        }
      ]
    },
    "views": {
      "socratic-shell": [
        {
          "id": "socratic-shell.walkthrough",
          "name": "Walkthrough",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "comments/commentThread/context": [
        {
          "command": "socratic-shell.addWalkthroughComment",
          "group": "inline@1",
          "when": "commentController == dialectic-walkthrough"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run webpack",
    "webpack": "webpack --mode production",
    "webpack-dev": "webpack --mode development",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "16.x",
    "@types/shell-quote": "^1.7.5",
    "@types/vscode": "^1.74.0",
    "ts-loader": "^9.4.0",
    "typescript": "^4.9.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "mermaid": "^10.9.1",
    "shell-quote": "^1.8.3"
  }
}